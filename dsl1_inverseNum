import java.util.*;

public class Main{

public static void main(String[] args) {
  // write your code here  
  Scanner scn = new Scanner(System.in);
  int n = scn.nextInt();
  int temp = n;
  int res = 0;
  for(int i = 1; i <= n; i++){
    int rem = temp % 10;
    temp = temp / 10;
    res = res + i * (int)Math.pow(10,rem - 1);


  }
  System.out.println(res);

 }
}
