import java.util.*;
   
   public class Main{
   
   public static void main(String[] args) {
     // write your code here  
     Scanner scn = new Scanner(System.in);
     int n = scn.nextInt();
     int temp = n;
     while(temp != 0){
         int quo = temp / 10;
         int rem = temp % 10;
         temp = quo;
         System.out.println(rem);
     }
    }
   }
